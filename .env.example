# Application Environment
NODE_ENV=development
GENERATE_SOURCEMAP=false
REACT_APP_ENV=development

# Service URLs (adjust ports/hosts as needed)
REACT_APP_AUTH_SERVICE_URL=http://localhost/api/auth
REACT_APP_API_URL=http://localhost/api/v1
REACT_APP_KEYCLOAK_URL=http://localhost/auth

# Keycloak Configuration
REACT_APP_KEYCLOAK_REALM=synthai
REACT_APP_KEYCLOAK_CLIENT_ID=synthai-frontend

# Feature Flags
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_MONITORING=false

# API Timeouts (in milliseconds)
REACT_APP_API_TIMEOUT=10000
REACT_APP_AUTH_TIMEOUT=5000

# Application Settings
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_TIME=development

NEXT_PUBLIC_AUTH_API_URL=http://localhost:3001/api
NEXT_PUBLIC_LOGIC_API_URL=http://localhost:8081/api/v1
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080
NEXT_PUBLIC_KEYCLOAK_REALM=synthai
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=synthai-frontend-client